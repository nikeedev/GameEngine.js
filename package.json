  "bugs": {
    "url": "https://github.com/nikeedev/playlib/issues"
  },
  "homepage": "https://github.com/nikeedev/playlib#readme",
  "devDependencies": {
    "@webpack-cli/generators": "^3.0.7",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
